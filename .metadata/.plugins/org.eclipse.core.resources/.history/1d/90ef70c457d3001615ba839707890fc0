package com.print.subset.array;

public class Test {

	public static void main(String[] args) {
		//String s = "abbabba";
		String s="aabcccdee";
		int i = 0;
		int j=i+1;
		while(i<=j){
		if (s.charAt(i) == s.charAt(j)) {
				i++;
				j++;
			}else{
				s = s.substring(j);
			}
		}
		System.out.println(s);
	}

}
