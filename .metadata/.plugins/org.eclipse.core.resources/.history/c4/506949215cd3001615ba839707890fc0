package com.print.subset.array;

public class Test {

	public static void main(String[] args) {
		// String s = "abbabba";
		String s = "aabcccdee";
		int i = 0;
		int j = i + 1;
		findSubString(s,i,j);	
		System.out.println(s);
	}

	private static void findSubString(String s, int i, int j) {
		while (s.charAt(i) == s.charAt(j)) {
			i++;
			j++;
			s = s.substring(j, s.length() - 1);
			i=0;
			j=i+1;
		
	}		
	}
	
	

}
