package com.print.subset.array;

public class Test {

	public static void main(String[] args) {
		String s = "abbabba";
		int i = 0;
		while (i >=0&& s.length()>1) {
			if (s.charAt(i) == s.charAt(i + 1)) {
				s = s.substring(0, i) + s.substring(i + 2);
				i=0;
			}else{
				i++;
			}
		}

		System.out.println(s);
	}

}
