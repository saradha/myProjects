package com.print.subset.array;

public class FindSubSetArrayMaMinDifference {

	public static void main(String[] args) {
		int num[]={3, 4, 1, 9, 56, 7, 9, 12};
		int n=num.length;
		for(int i=0;i<(1<<i);i++){
			System.out.print("{ ");
			for(int j=0;j<n;j++){
				if((i&(1<<j))>0){
					System.out.print(num[j]);
				}
			}
			System.out.println("}");
		}
	}

}
